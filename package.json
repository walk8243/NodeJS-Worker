{
  "name": "nodejs-worker",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn run tsc",
    "run:cluster": "yarn run build && yarn run start:cluster",
    "run:thread": "yarn run build && yarn run start:thread",
